QScrollBar:vertical {
      border: 2px solid rgb(242,242,242);
      background: rgb(242,242,242);
      width: 16px;
      margin: 16px 0 16px 0;
  }
QScrollBar::handle:vertical {
      background: rgb(217,217,217);
      min-height: 20px;
  }
QScrollBar::add-line:vertical {
      border: 0px solid red;
     
      height: 16px;
      subcontrol-position: bottom;
      subcontrol-origin: margin;
	  border-image:url(:/images/but_scroll_down.png);
  }

QScrollBar::sub-line:vertical {
      border: 0px solid red;
      
      height: 16px;
      subcontrol-position: top;
      subcontrol-origin: margin;
	  border-image:url(:/images/but_scroll_up.png);
  }

  /*scroll bar  hengde zj*/
QScrollBar:horizontal {
     border: 2px solid rgb(242,242,242);
      background: rgb(242,242,242);
      height: 16px;
      margin: 0px 16px 0px 16px;
 }
QScrollBar::handle:horizontal {
     background: rgb(217,217,217);
      min-height: 20px;
 }
QScrollBar::add-line:horizontal {
     border: 0px solid red;
     
      width: 16px;
      subcontrol-position: right;
      subcontrol-origin: margin;
	  border-image:url(:/images/but_scroll_right.png);
 }

 QScrollBar::sub-line:horizontal {
     border: 0px solid red;
      
      width: 16px;
      subcontrol-position: left;
      subcontrol-origin: margin;
	  border-image:url(:/images/but_scroll_left.png);
 }

	  
	  
	  
QTabWidget::pane { /* The tab widget frame */
     border:1px solid #C4C4C3;
 }

 QTabWidget::tab-bar {
     left: 0px; /* move to the right by 5px */
 }

 /* Style the tab using the tab sub-control. Note that
     it reads QTabBar _not_ QTabWidget */
 
 
 
 
 
 QTabWidget > QTabBar::tear {
     /*image: url(:/but_scroll_right.png);*/
 }

  QTabWidget > QTabBar::scroller { /* the width of the scroll buttons */
     width: 20px;
 }

 QTabWidget >  QTabBar QToolButton { /* the scroll buttons are tool buttons */
     border-image: url(:/images/QTabBar_scroller_bj.png) 2;
     border-width: 2px;
 }

  QTabWidget > QTabBar QToolButton::right-arrow { /* the arrow mark in the tool buttons */
     image: url(:/images/QTabBar_scroller_right.png);
 }

 QTabWidget > QTabBar QToolButton::left-arrow {
     image: url(:/images/QTabBar_scroller_left.png);
 }
 
 QTabWidget > QTabBar::tab {
     background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,stop: 0 #DFDFDF, stop: 0.5 #ffffff,  stop: 1.0 #EAEAEA
                                 );						 
     border-left: 1px solid #C4C4C3; 
	 border-top: 1px solid #C4C4C3; 
	 border-right: 1px solid #C4C4C3;
     border-top-left-radius: 4px;
     border-top-right-radius: 4px;
	 min-width:140px;
     padding:2px 15px 2px 15px;
	 height:23px;
	  margin-top: 2px; 
 }

  QTabWidget > QTabBar::tab:selected
 {
 margin-top: 0px; 
   height:25px;
     background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                 stop: 0.3 #ffffff,  stop: 1.0 #eeeeee);
   padding:2px 5px 2px 5px;
   min-width:160px;
  font-weight:bold;


}
  
 QTabWidget > QTabBar::tab:hover {
     background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                 stop: 0 #DFDFDF, stop: 0.5 #ffffff);
  color:rgb(50,50,50);
  }



  QTabWidget > QTabBar::tab:only-one {
     margin: 0; /* if there is only one tab, we don't want overlapping margins */
 }
 
 
