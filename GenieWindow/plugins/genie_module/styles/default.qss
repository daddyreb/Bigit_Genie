/*COMBOBOX LANGUAGE*/
/*combobox*/
QFrame [stylehintcob="1"]{
color: rgb(100,100,100);
background-color:rgb(255,255,241);
selection-color: #ffffff;
selection-background-color: rgb(191,31,127);
border:2px solid gray;
border-radius:5px;
	padding-left:8px;
	padding-right:8px;
	margin-right:0px;
	margin-left:0px;
}
QFrame [stylehintcob="1"]:hover{
	border:2px solid #9E99FD;
}
QFrame [stylehintcob="1"]:checked{
color: rgb(100,100,100);

}

QFrame [stylehintcob="1"]::drop-down{
	background-image: url(:/but_combo.png) 2;
		border-radius:2px;
	subcontrol-origin: margin;
	subcontrol-position: center right;
border-width:0px;
	width: 30px;

}

QFrame [stylehintcob="1"]::indicator{
	width: 20;
	height: 23px;
	color: #ff0000;
}


