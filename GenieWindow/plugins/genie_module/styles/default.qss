/*update button   zj*/
QPushButton#but_update
{
	padding-left:25px;
	padding-right:18px;
	border-style:solid;
/*	width:130px;*/
	height:17px;
	border-width:1px;
	border-radius: 8px;
	background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #FFA86E, stop:1 #ff6600);
	color:rgb(255,255,255);
	font-size:12px;
	font-weight:bold;
	border:1px solid #ff6600;
	 background-image: url(:/ico_update_genie.png);
	 background-repeat: none;
}

QPushButton#but_update:hover
{
	background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #ffffff, stop:1 #ff6600);
	color:#ffffff;
	border:1px solid #938FDC;
}
QPushButton#but_update:pressed
{
	background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #ff6600, stop:1 #ffffff);
	color:rgb(255,255,255);
	border:1px solid #DFDDF4;
}


